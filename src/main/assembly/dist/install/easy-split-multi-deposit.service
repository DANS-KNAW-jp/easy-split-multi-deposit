[Unit]
Description=EASY Split Multi Deposit

[Service]
ExecStart=/bin/java \
 -Dlogback.configurationFile=/etc/opt/dans.knaw.nl/easy-split-multi-deposit/logback-service.xml \
 -Dapp.home=/opt/dans.knaw.nl/easy-split-multi-deposit \
 -jar /opt/dans.knaw.nl/easy-split-multi-deposit/bin/easy-split-multi-deposit.jar run-service

User=easy-split-multi-deposit
Group=easy-split-multi-deposit

[Install]
WantedBy=multi-user.target
